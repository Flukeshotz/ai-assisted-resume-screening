    {
      "parameters": {
        "schemaType": "manual",
        "inputSchema": "{\n  \"type\": \"object\",\n  \"properties\": {\n    \"overall_score\": {\"type\": \"number\"},\n    \"score_breakdown\": {\n      \"type\": \"object\",\n      \"properties\": {\n        \"skills_match\": {\n          \"type\": \"object\",\n          \"properties\": {\n            \"score\": {\"type\": \"number\"},\n            \"reasoning\": {\"type\": \"string\"}\n          }\n        },\n        \"experience_or_projects\": {\n          \"type\": \"object\",\n          \"properties\": {\n            \"score\": {\"type\": \"number\"},\n            \"reasoning\": {\"type\": \"string\"}\n          }\n        },\n        \"domain_alignment\": {\n          \"type\": \"object\",\n          \"properties\": {\n            \"score\": {\"type\": \"number\"},\n            \"reasoning\": {\"type\": \"string\"}\n          }\n        },\n        \"impact\": {\n          \"type\": \"object\",\n          \"properties\": {\n            \"score\": {\"type\": \"number\"},\n            \"reasoning\": {\"type\": \"string\"}\n          }\n        },\n        \"resume_quality\": {\n          \"type\": \"object\",\n          \"properties\": {\n            \"score\": {\"type\": \"number\"},\n            \"reasoning\": {\"type\": \"string\"}\n          }\n        }\n      }\n    },\n    \"why_good_fit\": {\"type\": \"string\"},\n    \"concerns_to_probe\": {\"type\": \"array\", \"items\": {\"type\": \"string\"}},\n    \"evaluation_confidence\": {\"type\": \"string\", \"enum\": [\"high\", \"medium\", \"low\"]}\n  },\n  \"required\": [\"overall_score\", \"score_breakdown\", \"why_good_fit\", \"concerns_to_probe\", \"evaluation_confidence\"]\n}"
      },
      "id": "435b55e8-010d-4d1d-9328-f1066662e731",
      "name": "Scoring Schema Parser",
      "type": "@n8n/n8n-nodes-langchain.outputParserStructured",
      "typeVersion": 1.3,
      "position": [
        1832,
        680
      ]
    },
    {
      "parameters": {
        "jsCode": "const data = $input.first().json;\nconst score = data.overall_score;\nconst parseConfidence = data.parse_confidence || \"high\";\nconst classification = data.classification;\nconst evalConfidence = data.evaluation_confidence;\n\n// Human review required if:\n// - Parse confidence is low\n// - Score is in mid-range (40-60)\n// - Candidate type is ambiguous\n// - Evaluation confidence is low\nconst humanReviewRequired = \n  parseConfidence === \"low\" ||\n  (score >= 40 && score <= 60) ||\n  classification === \"ambiguous\" ||\n  evalConfidence === \"low\";\n\nreturn [{\n  json: {\n    ...data,\n    human_review_required: humanReviewRequired ? \"Yes\" : \"No\"\n  }\n}];"
      },
      "id": "05dbda3c-660b-4430-9dc7-dbc3e9991783",
      "name": "Determine Human Review Flag",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        2048,
        456
      ]
    },
    {
      "parameters": {
        "jsCode": "const data = $input.first().json;\nconst resumeFileId = $node[\"Watch Resume Folder\"].json.id;\nconst resumeFileLink = `https://drive.google.com/file/d/${resumeFileId}/view`;\nconst jdVersion = data.jd_id || \"unknown\";\n\nreturn [{\n  json: {\n    resume_file_id: resumeFileId,\n    candidate_name: data.name || \"Unknown\",\n    email: data.email || \"\",\n    phone: data.phone || \"\",\n    resume_file_link: resumeFileLink,\n    candidate_type: data.classification || \"unknown\",\n    overall_score: data.overall_score || 0,\n    skill_match_score: data.score_breakdown?.skills_match?.score || 0,\n    experience_project_score: data.score_breakdown?.experience_or_projects?.score || 0,\n    impact_score: data.score_breakdown?.impact?.score || 0,\n    resume_quality_score: data.score_breakdown?.resume_quality?.score || 0,\n    why_good_fit: data.why_good_fit || \"\",\n    concerns_to_probe: (data.concerns_to_probe || []).join(\"; \"),\n    evaluation_confidence: data.evaluation_confidence || \"low\",\n    human_review_required: data.human_review_required || \"Yes\",\n    jd_version: jdVersion,\n    last_evaluated: new Date().toISOString()\n  }\n}];"
      },
      "id": "de24593e-7205-4f55-a349-9b1e7e78b38d",
